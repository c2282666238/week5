package week5.subjectList;

public class classList {
private int year,semester;
private subject sub;
private student s;
public classList() {
	
}
public classList(int year, int semester, subject sub, student s) {
	setYear( year);
	setSemester(semester);
	setSub(sub);
	sets( s);
}
public int getYear() {
	return year;
}
public void setYear(int year) {
	this.year = year;
}
public int getSemester() {
	return semester;
}
public void setSemester(int semester) {
	this.semester = semester;
}
public subject getSub() {
	return sub;
}
public void setSub(subject sub) {
	this.sub = sub;
}
public student gets() {
	return s;
}
public void sets(student s) {
	this.s = s;
}

@Override
public String toString() {
	return this.year+"学年"+this.semester+"学期\n"+
           this.sub.toString()+"\n"+
           "学号\t姓名\n"+
           this.s.toString()+"\n";
}
}
